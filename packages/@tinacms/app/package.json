{
  "name": "@tinacms/app",
  "version": "0.0.21",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    }
  },
  "typings": "dist/index.d.ts",
  "license": "Apache-2.0",
  "buildConfig": {
    "entryPoints": [
      {
        "name": "src/index.ts",
        "target": "node"
      }
    ]
  },
  "scripts": {
    "types": "rm dist/index.d.ts && touch dist/index.d.ts && echo \"export declare const viteBuild: (args: any) => any\" > dist/index.d.ts",
    "build": "tinacms-scripts build",
    "inline": "cd appFiles && npm --prefix ./ i --global-style --legacy-peer-deps --no-package-lock --production"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.1",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@tinacms/scripts": "workspace:*",
    "jest": "^27.0.6"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tailwindcss/typography": "^0.5.4",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.0",
    "fs-extra": "^9.0.1",
    "postcss": "^8.4.14",
    "postcss-nested": "^5.0.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-is": "18.2.0",
    "react-router-dom": "6",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.6.4",
    "vite": "^3.1.3"
  }
}
